{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node",
        "config": {
          "includeFiles": [
            "package.json"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  